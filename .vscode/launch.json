    "version": "0.2.0",
    "configurations": [
        {
            "name": "PowerShell: Repo list",
            "type": "PowerShell",
            "request": "launch",
            "script": ".\\powershell\\repos-list.ps1 ",
            "cwd": "${workspaceFolder}",
            "args": ["-organisation \"[ORG]\" -personalAccessToken \"[ACCESS_KEY]\""]
        },
        {
            "name": "PowerShell: Releases list",
            "type": "PowerShell",
            "request": "launch",
            "script": ".\\powershell\\releases-list.ps1 ",
            "cwd": "${workspaceFolder}",
            "args": ["-organisation \"[ORG]\" -personalAccessToken \"[ACCESS_KEY]\""]
        },
        {
            "name": "PowerShell: Pipelines list",
            "type": "PowerShell",
            "request": "launch",
            "script": ".\\powershell\\pipelines-list.ps1 ",
            "cwd": "${workspaceFolder}",
            "args": ["-organisation \"[ORG]\" -personalAccessToken \"[ACCESS_KEY]\""]
        }
    ]
}